! Xft
Xft.dpi: 168.0
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting: true
Xft.antialias: true
Xft.rgba: rgb


! URxvt

! Font
URxvt.font: xft:Iosevka:hinting=True:size=8:style=Regular:antialias=True

! General
URxvt.depth: 32
URxvt.loginShell: true
URxvt.internalBorder: 5
URxvt.lineSpace: 0
URxvt.letterSpace: 2
URxvt.clipboard.autocopy: true
URxvt.clipboard.copycmd: xsel -i -b
URxvt.clipboard.pastecmd: xsel -b
URxvt*scrollTtyOutput: false
URxvt*scrollWithBuffer: true
URxvt*scrollTtyKeypress: true
URxvt*saveLines: 10000
URxvt*visualBell: true
URxvt*urgentOnBell: true
URxvt*cutchars: "\"(),<>[]{}|'`Â¬"

! Scrollbar
URxvt.scrollStyle: rxvt
URxvt.scrollBar: false

! Extensoins
URxvt.perl-ext-common: default,clipboard,matcher,keyboard-select,confirm-paste
URxvt.perl-ext: url-select
URxvt.url-select.launcher: firefox
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.matcher.launcher.0: xdg-open
URxvt.urlLauncher: xdg-open
URxvt.matcher.button: 1
URxvt.keysym.C-Delete: matcher:last
URxvt.keysym.M-Delete: matcher:list
URxvt.keysym.C-M-Up: font-size:incglobal
URxvt.keysym.C-M-Down: font-size:decglobal

! Colors
! 593nm
#define amber            #ffcc00
! 600nm
#define dark_amber       #ffb000
! other amber
#define amber2           #cc8505
#define crt_green        #50fa7b

! Dracula Xresources palette
*.foreground: #F8F8F2
*.background: #282A36
! black
*.color0:     #000000
*.color8:     #4D4D4D
! red
*.color1:     #FF5555
*.color9:     #FF6E67
! green
*.color2:     #50FA7B
*.color10:    #5AF78E
! yellow
*.color3:     #F1FA8C
*.color11:    #F4F99D
! blue
*.color4:     #BD93F9
*.color12:    #CAA9FA
! magenta
*.color5:     #FF79C6
*.color13:    #FF92D0
! cyan
*.color6:     #8BE9FD
*.color14:    #9AEDFE
! white
*.color7:     #BFBFBF
*.color15:    #E6E6E6

urxvt*background:        #1c1c1c

! rofi
rofi.color-enabled: true
!                  bg          border     separator
rofi.color-window: #282a36,    #8BE9FD,   #40FA7B
!                  bg          fg         bg-alt      hl-bg      hl-fg
rofi.color-normal: #282a36,    #9AEDFE,   #282a36,    #9AEDFE,   #000000
rofi.color-active: #282a36,    #FF92D0,   #282a36,    #FF79C6,   #000000
rofi.color-urgent: #282a36,    #FF5555,   #282a36,    #FF5555,   #F1FA8C

rofi.separator-style: solid
rofi.sidebar-mode: false
rofi.columns: 1
rofi.lines: 15
rofi.width: 50
rofi.bw: 1
rofi.padding: 16
rofi.yoffset: 0
rofi.location: 0
rofi.font: Hack 12
rofi.fixed-num-lines: true
