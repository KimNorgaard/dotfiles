! Xft
Xft.dpi: 168.0
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting: true
Xft.antialias: true
Xft.rgba: rgb


! URxvt

! Font
URxvt.font: xft:Iosevka:hinting=True:size=8:style=Regular:antialias=True

! General
URxvt.depth: 32
URxvt.loginShell: true
URxvt.internalBorder: 3
URxvt.lineSpace: 0
URxvt.letterSpace: 2
URxvt.clipboard.autocopy: true
URxvt.clipboard.copycmd: xsel -i -b
URxvt.clipboard.pastecmd: xsel -b
URxvt*scrollTtyOutput: false
URxvt*scrollWithBuffer: true
URxvt*scrollTtyKeypress: true
URxvt*saveLines: 10000
URxvt*visualBell: true
URxvt*urgentOnBell: true

! Scrollbar
URxvt.scrollStyle: rxvt
URxvt.scrollBar: false

! Extensoins
URxvt.perl-ext-common: default,clipboard,matcher,keyboard-select,confirm-paste
URxvt.perl-ext: url-select
URxvt.url-select.launcher: firefox
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.matcher.launcher.0: xdg-open
URxvt.urlLauncher: xdg-open
URxvt.matcher.button: 1
URxvt.keysym.C-Delete: matcher:last
URxvt.keysym.M-Delete: matcher:list
URxvt.keysym.C-M-Up: font-size:incglobal
URxvt.keysym.C-M-Down: font-size:decglobal

! Colors
! 593nm
#define amber            #ffcc00
! 600nm
#define dark_amber       #ffb000
#define crt_green        #50fa7b
#define dark_black       #373b41
#define light_black      #000000
#define dark_red         #a54242
#define light_red        #cc6666
#define dark_green       #8c9440
#define light_green      #b5bd68
#define dark_yellow      #de935f
#define light_yellow     #f0c674
#define dark_blue        #5f819d
#define light_blue       #81a2be
#define dark_magenta     #85678f
#define light_magenta    #b294bb
#define dark_cyan        #5e8d87
#define light_cyan       #8abeb7
#define dark_white       #707880
#define light_white      #c5c8c6
#define gray15           #262626
!#define gray16           #262626

*foreground:             amber
!*background:             gray15
!*background:             #221100
*background:             #111110

! black
*color0:                 dark_black
*color8:                 light_black

! red
*color1:                 dark_red
*color9:                 light_red

! green
*color2:                 dark_green
*color10:                light_green

! yellow
*color3:                 dark_yellow
*color11:                light_yellow

! blue
*color4:                 dark_blue
*color12:                light_blue

! magenta
*color5:                 dark_magenta
*color13:                light_magenta

! cyan
*color6:                 dark_cyan
*color14:                light_cyan

! white
*color7:                 dark_white
*color15:                light_white


! rofi
rofi.color-enabled: true
!                  bg          border     separator
rofi.color-window: gray15,     crt_green, #a89984
!                  bg          fg         bg-alt      hl-bg      hl-fg
rofi.color-normal: gray15,     crt_green, gray15,     crt_green, gray15
rofi.color-active: dark_amber, gray15,    dark_amber, amber,     gray15
rofi.color-urgent: gray15,     dark_red,  gray15,     gray15,    light_red

rofi.separator-style: solid
rofi.sidebar-mode: false
rofi.columns: 1
rofi.lines: 15
rofi.width: 50
rofi.bw: 1
rofi.padding: 16
rofi.yoffset: 0
rofi.location: 0
rofi.font: Hack 12
rofi.fixed-num-lines: true
