! rofi {{{
rofi.color-enabled:        true
!                          bg          border     separator
rofi.color-window:         #282a36,    #8BE9FD,   #40FA7B
!                          bg          fg         bg-alt      hl-bg      hl-fg
rofi.color-normal:         #282a36,    #9AEDFE,   #282a36,    #9AEDFE,   #000000
rofi.color-active:         #282a36,    #FF92D0,   #282a36,    #FF79C6,   #000000
rofi.color-urgent:         #282a36,    #FF5555,   #282a36,    #FF5555,   #F1FA8C

rofi.separator-style:      solid
rofi.sidebar-mode:         false
rofi.columns:              1
rofi.lines:                15
rofi.width:                50
rofi.bw:                   1
rofi.padding:              16
rofi.yoffset:              0
rofi.location:             0
rofi.font:                 Hack 12
rofi.fixed-num-lines:      true
! }}}
